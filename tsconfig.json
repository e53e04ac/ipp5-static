{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "lib": [
            "dom",
            "es2017",
        ]
    },
    "exclude": [
        "node_modules/",
        "thirdparty/"
    ]
}